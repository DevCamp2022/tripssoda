<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        
<mapper namespace="com.devcamp.tripssoda.mapper.UserHistoryMapper">
    <insert id="insertUserHistory" parameterType="UserHistoryDto">
        INSERT INTO user_history (user_id, detail, updated_by)
        VALUES (#{userId}, #{detail}, #{updatedBy})
    </insert>
</mapper>